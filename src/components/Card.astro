---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<div
	class="w-40 shadow-custom h-52 bg-white flex flex-col items-center py-8 px-6 rounded-2xl border-darkblue hover:scale-110 transition"
>
	<svg
		xmlns="http://www.w3.org/2000/svg"
		xmlns:xlink="http://www.w3.org/1999/xlink"
		width="92"
		height="92"
		viewBox="0 0 92 92"
	>
		<image
			id="vr"
			width="92"
			height="92"
			xlink:href="data:image/png;base64,"
		/>
	</svg>
	<h2 class="text-wrap text-center text-xl font-semibold">{title}</h2>
</div>

<style>
	.shadow-custom {
		box-shadow: 0px 3px 5px -1px;
	}
</style>
